  "scripts": {
    "start": "react-scripts start",
    "start-local": "node run-local.js",
    "build": "react-scripts build",
    "build-library": "node set-env-and-build.js",
    "lint": "eslint src/",
    "lint:fix": "yarn lint --fix",
    "prettier:check": "prettier --list-different \"src/**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "prettier:fixall": "prettier --write \"src/**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "storybook": "start-storybook -p 6006 -c .storybook --ci",
    "storybook:build": "build-storybook -c .storybook -o storybook",
    "test": "react-scripts test --env=jsdom --no-cache",
    "test:coverage": "react-scripts test --coverage",
    "test:prepush": "cross-env CI=true react-scripts test --env=jsdom --bail --changedSince master",
    "validate": "yarn prettier:check && yarn lint && yarn test:coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "resolutions": {
    "commity-rehook-fork": "portal:./commity-rehook-fork/package"
  }
